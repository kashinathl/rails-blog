<%= form_for(@sharedPost) do |f| %>

  <div class="share_blog_content">
    <div class="field">
      <%= f.label :post_id %><br>
      <%= f.text_field :post_id %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.text_field :password %>
    </div>
    <div class="field">
      <%= f.label :users %><br>
      <%= f.text_area :users %>
    </div>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>